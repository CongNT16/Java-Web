<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>err</param-name>
        <param-value>Choosse an option</param-value>
    </context-param>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>Controller.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addMovie</servlet-name>
        <servlet-class>Controller.addMovie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listMovie</servlet-name>
        <servlet-class>Controller.listMovie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>list</servlet-name>
        <servlet-class>Controller.listMovie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>Controller.delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>book1</servlet-name>
        <servlet-class>Controller.book1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>book2</servlet-name>
        <servlet-class>Controller.book2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>Controller.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookingHistory</servlet-name>
        <servlet-class>Controller.bookingHistory</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addMovie</servlet-name>
        <url-pattern>/addMovie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listMovie</servlet-name>
        <url-pattern>/sucess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>list</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>book1</servlet-name>
        <url-pattern>/book1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>book2</servlet-name>
        <url-pattern>/book2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookingHistory</servlet-name>
        <url-pattern>/bookingHistory</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
