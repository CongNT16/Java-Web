<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>FapFPT</servlet-name>
        <servlet-class>FapFpt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.UserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>controller.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>change</servlet-name>
        <servlet-class>controller.Change</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fee</servlet-name>
        <servlet-class>controller.Coursefee</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>attendance</servlet-name>
        <servlet-class>controller.Attendance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>attencourse</servlet-name>
        <servlet-class>controller.CourseAtten</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TableTime</servlet-name>
        <servlet-class>controller.TableTime</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FapFPT</servlet-name>
        <url-pattern>/fapfpt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>change</servlet-name>
        <url-pattern>/change</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fee</servlet-name>
        <url-pattern>/fee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>attendance</servlet-name>
        <url-pattern>/attendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>attencourse</servlet-name>
        <url-pattern>/attencourse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TableTime</servlet-name>
        <url-pattern>/TableTime</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
